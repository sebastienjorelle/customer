        <formula>IF(

First_Response_Sent_DateTime__c &lt; First_Response_Due__c , &quot;OK - sent before due&quot;,
   
IF(AND(TimeTillSLA__c &gt; 20,First_Response_Sent__c = false) , &quot;OK - date not passed&quot;, 

IF(AND(AND(TimeTillSLA__c &gt;0, TimeTillSLA__c &lt;= 20), First_Response_Sent__c = false), &quot;WARNING - SLA Near&quot;, 

IF(AND(TimeTillSLA__c &lt; 0, First_Response_Sent__c = false), &quot;Violated - not sent&quot;,

IF(First_Response_Sent_DateTime__c &gt; First_Response_Due__c, &quot;Violated - sent after due&quot;, 
  
&quot;UNKNOWN&quot;)))))</formula>
