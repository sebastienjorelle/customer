    <validationRules>
        <fullName>Can_not_reassign_case</fullName>
        <active>true</active>
        <description>triggers if a CSR tries to re-assign a case he did not work on</description>
        <errorConditionFormula>AND (
     ISPICKVAL( Status , &quot;In Progress&quot;), 
     ISCHANGED( OwnerId ),
     $User.ProfileId = &quot;00eg0000000DqLG&quot;
     )</errorConditionFormula>
        <errorMessage>You can not change the owner of a case you have not worked on</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_Subject_Description_read_only</fullName>
        <active>true</active>
        <description>Case Subject/Description are read only if the origin of the case is webform</description>
        <errorConditionFormula>AND( 
   OR( ISPICKVAL(Origin, &quot;Web form&quot;), 
       ISPICKVAL (Origin, &quot;Email&quot;)),
   OR ( ISCHANGED( Subject ),
        ISCHANGED( Description )  
       )
    )</errorConditionFormula>
        <errorMessage>You are not allowed to modify the case subject and/or description</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Reason_Mandatory</fullName>
        <active>true</active>
        <description>this validation rule triggers if a case is closed without a case reason selected</description>
        <errorConditionFormula>AND
    (
      ISPICKVAL( Status , &quot;Closed&quot; ),
      ISPICKVAL(  Case_Close_Reason__c , &quot;&quot; ) 
     )</errorConditionFormula>
        <errorDisplayField>Case_Close_Reason__c</errorDisplayField>
        <errorMessage>Please select a close reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OS_Mandatory_for_Devices</fullName>
        <active>true</active>
        <description>OS mandatory if Device chosen</description>
        <errorConditionFormula>AND (
NOT(ISPICKVAL ( Device__c , &quot;&quot;)),
ISPICKVAL ( OS__c, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>OS__c</errorDisplayField>
        <errorMessage>You must enter OS information if Device is specified</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Open_actions</fullName>
        <active>true</active>
        <description>triggers if a user tries to close a case with open actions</description>
        <errorConditionFormula>AND (
      ISPICKVAL(  Status , &quot;Closed&quot;),
      Open_Actions__c &gt; 0
     )</errorConditionFormula>
        <errorMessage>You cannot close a case with open action(s), please complete the pending action(s) before closing the case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_device_description</fullName>
        <active>true</active>
        <description>if device=other, other device info field is mandatory</description>
        <errorConditionFormula>AND (
      ISPICKVAL( Device__c , &quot;Other&quot;),
      ISBLANK( Other_device_description__c )  
     )</errorConditionFormula>
        <errorDisplayField>Other_device_description__c</errorDisplayField>
        <errorMessage>You have selected other device, please enter a device descritpion</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_error_code</fullName>
        <active>true</active>
        <description>if error code=other, other error code is mandatory</description>
        <errorConditionFormula>AND (
      ISPICKVAL(  Error_Code__c , &quot;Other&quot;),
      ISBLANK(  Other_Error_Code__c  )  
     )</errorConditionFormula>
        <errorDisplayField>Other_Error_Code__c</errorDisplayField>
        <errorMessage>You have selected other error code, please enter an error code</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reopen_close_case_outside_timeframe</fullName>
        <active>true</active>
        <description>triggers if a user tries to reopen a case outside of the reopening period</description>
        <errorConditionFormula>AND ( PRIORVALUE( Status )= &quot;Closed&quot;,
        Re_open_limit__c  &lt; NOW()
     )</errorConditionFormula>
        <errorMessage>This case is outside of the 14 days reopening period, please create a new case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reopen_reason</fullName>
        <active>true</active>
        <description>if status = re-open, re-open reason is mandatory before changing the status</description>
        <errorConditionFormula>AND (
     ISPICKVAL(PRIORVALUE( Status ), &quot;Reopened&quot;), 
     ISPICKVAL( Reopen_Reason__c , &quot;&quot;) 
     )</errorConditionFormula>
        <errorDisplayField>Reopen_Reason__c</errorDisplayField>
        <errorMessage>This case was re-opened please select a re-open reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Technical_Issues_Details</fullName>
        <active>true</active>
        <description>Tech Issue cases require technical details input</description>
        <errorConditionFormula>AND(  
      OR(ISPICKVAL(Status, &quot;Closed&quot;), 
         ISPICKVAL (Status, &quot;Pending - escalated&quot;)
         ),
     ISPICKVAL ( Category__c , &quot;Technical Issues&quot;),
     ISPICKVAL ( Device__c , &quot;&quot;)
)</errorConditionFormula>
        <errorMessage>Please complete all further technical information (device, OS)</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Digital_Locker</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Digital Locker</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/support/console/30.0/integration.js&quot;)} 
if({!$Permission.ViewLocker}){
if (sforce.console.isInConsole()){
srcUp(&apos;/apex/GEN_DigitalLocker?isdtp=vw&amp;caseId={!Case.Id}&apos;);
}
else{
window.location = &apos;/apex/GEN_DigitalLocker?caseId={!Case.Id}&apos;;
}
}
else{
alert(&apos;You do not have authorized permission to perform this action&apos;);
}</url>
    </webLinks>
