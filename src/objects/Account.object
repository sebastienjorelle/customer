    <validationRules>
        <fullName>Account_name_read_only</fullName>
        <active>true</active>
        <description>Account name is read only for all users but API user and system administrator</description>
        <errorConditionFormula>AND ( 
     NOT ( OR (
               $Profile.Name = &quot;API User&quot;,
               $Profile.Name = &quot;System Administrator&quot;
               )
           ),
     OR (ISCHANGED( FirstName ),
         ISCHANGED( LastName )
         )
     )</errorConditionFormula>
        <errorMessage>You do not have the permission to change the customer&apos;s name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Twitter_Handle_format</fullName>
        <active>true</active>
        <description>trigger if the handle does not start with @</description>
        <errorConditionFormula>NOT
   (
    BEGINS 
          ( Twitter_handle__c , &quot;@&quot; )
    )</errorConditionFormula>
        <errorDisplayField>Twitter_handle__c</errorDisplayField>
        <errorMessage>Twitter Handle should start with an &quot;@&quot;</errorMessage>
    </validationRules>
